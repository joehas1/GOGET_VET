

<div class="vet-container">
<!--   <div class="btn-transparent">
 --><button class="btn-transparent homepage-btn"><%=link_to "HOMEPAGE", root_path%></button>
    <button class="btn-transparent refresh-btn">REFRESH VETS</button>
  <!-- </div> -->
  <div id="viewport">
    <ul class="stack">
      <% @vets.each do |vet| %>
        <li data-vet-id="<%= vet.id %>">
          <!-- <input type="hidden" name="Vet" id="vet" value="<%= vet.id %>"> -->
          <div class="doctor-profile-card">
            <h3>Doctor's Profile</h3>
            <img src="data:image/jpeg;base64," class="doctor-picture">
            <div class="profile-card-info">
              <h2>Dr. <%= vet.user.first_name %></h2>
              <p>Price: <%= vet.rate %>
                <br>
                Address: <%= vet.address %>
                <br>
                Languages spoken: <%= vet.languages %>
                 <div class="btn-transparent more-info-btn">
                MORE INFO
                </div>
              </p>
              <div class="arrows">
                <i class="material-icons arrowleft">keyboard_arrow_left</i>
                <i class="material-icons arrowright">keyboard_arrow_right</i>
              </div>
            </div>
          </div>
        </li>
      <%end%>
    </ul>
  </div>
</div>

<%= javascript_pack_tag 'swiping' %>














